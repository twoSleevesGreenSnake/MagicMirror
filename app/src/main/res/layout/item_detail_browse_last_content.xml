<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/item_browse_last_first_relayout_marginTop">

        <TextView
            android:id="@+id/item_detail_browse_last_content_purchase_consult_title_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="@string/item_browse_last_first_title_tv"
            android:textColor="#333"
            android:textSize="36px" />

        <ImageView
            android:id="@+id/item_detail_browse_last_content_purchase_consult_iv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/item_detail_browse_last_content_purchase_consult_title_tv"
            android:layout_marginTop="@dimen/item_detail_browse_last_content_purchase_consult_title_tv_marginTop"
            android:src="@mipmap/black_horizontal_stripe" />

        <TextView
            android:id="@+id/item_detail_browse_last_content_purchase_consult_content_tv"
            android:text="@string/item_detail_browse_last_content_purchase_consult_content_tv"
            android:layout_below="@+id/item_detail_browse_last_content_purchase_consult_iv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/item_detail_browse_last_content_purchase_consult_content_tv_marginTop"
            android:maxEms="15"
            android:layout_centerInParent="true"
            android:textColor="#333"
            android:textSize="26px"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/item_browse_last_second_relayout_marginTop">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/item_detail_browse_last_content_tract_order_title_tv"
            android:textSize="36px"
            android:textColor="#333"
            android:gravity="center"
            android:id="@+id/item_detail_browse_last_content_tract_order_title_tv"/>
        <ImageView
            android:id="@+id/item_detail_browse_last_content_tract_order_iv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/item_detail_browse_last_content_tract_order_title_tv"
            android:layout_marginTop="38px"
            android:src="@mipmap/black_horizontal_stripe" />

        <TextView
            android:id="@+id/item_detail_browse_last_content_purchase_consult_tract_order_tv"
            android:text="@string/item_detail_browse_last_content_purchase_consult_tract_order__tv"
            android:layout_below="@+id/item_detail_browse_last_content_tract_order_iv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/item_detail_browse_last_content_purchase_consult_tract_order_tv_marginTop"
            android:maxEms="15"
            android:layout_centerInParent="true"
            android:textColor="#333"
            android:textSize="26px"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/item_browse_last_third_relayout_marginTop">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/item_detail_browse_last_content_about_return_title_tv"
            android:textSize="36px"
            android:textColor="#333"
            android:gravity="center"
            android:id="@+id/item_detail_browse_last_content_about_return_title_tv"/>
        <ImageView
            android:id="@+id/item_detail_browse_last_content_about_return_iv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/item_detail_browse_last_content_about_return_title_tv"
            android:layout_marginTop="@dimen/item_detail_browse_last_content_about_return_iv"
            android:src="@mipmap/black_horizontal_stripe" />

        <TextView
            android:id="@+id/item_detail_browse_last_content_purchase_about_return_iv"
            android:text="@string/item_detail_browse_last_content_purchase_about_return_iv"
            android:layout_below="@+id/item_detail_browse_last_content_about_return_iv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/item_detail_browse_last_content_purchase_about_return_iv_marginTop"
            android:maxEms="15"
            android:layout_centerInParent="true"
            android:textColor="#333"
            android:textSize="26px"/>
    </RelativeLayout>
</LinearLayout>