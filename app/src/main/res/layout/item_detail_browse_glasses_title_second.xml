<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/item_detail_browse_glasses_rv_title_second_tv"
    android:layout_marginLeft="@dimen/item_detail_brose_glasses_rv_title_tv_marginLeft"
    android:layout_marginTop="@dimen/item_detail_brose_glasses_rv_title_tv_marginTop"
    android:textColor="@color/colorLight_Black"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/line_of_create_account"
        android:layout_below="@id/item_detail_browse_glasses_rv_title_second_tv"
        android:layout_marginTop="@dimen/item_detail_brose_glasses_rv_line_marginTop"
        android:layout_marginLeft="@dimen/item_detail_brose_glasses_rv_line_marginLeft"
        android:layout_marginRight="@dimen/item_detail_brose_glasses_rv_line_marginRight"
        android:id="@+id/item_detail_browse_glasses_rv_title_second_line"/>

</com.zhy.autolayout.AutoRelativeLayout>
